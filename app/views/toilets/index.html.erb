<% title "Toilets" %>

<table>
  <tr>
    <th>Street Address</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Rating</th>
    <th>Lat</th>
    <th>Lng</th>
  </tr>
  <% for toilet in @toilets %>
    <tr>
      <td><%=h toilet.street-address %></td>
      <td><%=h toilet.city %></td>
      <td><%=h toilet.state %></td>
      <td><%=h toilet.zip %></td>
      <td><%=h toilet.rating %></td>
      <td><%=h toilet.lat %></td>
      <td><%=h toilet.lng %></td>
      <td><%= link_to "Show", toilet %></td>
      <td><%= link_to "Edit", edit_toilet_path(toilet) %></td>
      <td><%= link_to "Destroy", toilet, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Toilet", new_toilet_path %></p>
